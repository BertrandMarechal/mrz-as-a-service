{
    "compilerOptions": {
      "preserveConstEnums": true,
      "strictNullChecks": true,
      "sourceMap": true,
      "outDir": ".build",
      "moduleResolution": "node",
      "lib": ["es2015"],
      "rootDir": "./",
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "types": ["node"],
      "allowSyntheticDefaultImports": true
    }
  }

  